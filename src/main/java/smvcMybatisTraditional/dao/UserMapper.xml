<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="smvcMybatisTraditional.dao.UserDao">
	<select id="getUserByName" parameterType="string" resultType="smvcMybatisTraditional.entity.User">
		SELECT * FROM sys_user u WHERE u.username = #{name}
	</select>
</mapper>
